package java.exam.entities;

import java.util.Date;

public class Freelance extends Citizen{
	private String Job;
	
	public Freelance(String citizenID, String name, String address, String phone, Date pCRDate,int type, String job) {
		super(citizenID, name, address, phone, pCRDate, type);
		Job = job;
	}

	public String getJob() {
		return Job;
	}

	public void setJob(String job) {
		Job = job;
	}
	
	
}
